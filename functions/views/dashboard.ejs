    <div class="hero-area-l-14 position-relative z-index-1 overflow-hidden" style="text-align: center;">
      <div class="container">
        <div class="row position-relative justify-content-center">
          <div class="clock" data-aos="fade-left" style="font-family:Futura; text-shadow: 4px 4px #000000; font-size: 120;">
            <div class="hours">
              <div class="first">
                <div class="number">0</div>
              </div>
              <div class="second">
                <div class="number">0</div>
              </div>
            </div>
            <div class="tick">:</div>
            <div class="minutes">
              <div class="first">
                <div class="number">0</div>
              </div>
              <div class="second">
                <div class="number">0</div>
              </div>
            </div>
          </div>
          
          <div class="col-xl-7 col-lg-5 col-md-8" data-aos="fade-right" data-aos-duration="800" data-aos-once="true">
            <div>
              <br>
              <p style="font-family:AppleGothic; font-size: 1.25rem; text-align:center;">Actions speak louder than words!</p>
              <br>
              <a style="background-color: transparent; border-width: 0px;" data-toggle="collapse" href="#collapseTimer" role="button" aria-expanded="false" aria-controls="collapseTimer">
                  <img id="startBtn" alt="Start a tomato" src="./public/image/button_start.png" width="348" height="96" onmouseover="startOver();" onmouseout="startOut();" onclick="startTomato();" ondblclick="startTomato();">
              </a>
            </div>
          </div>
        </div>
      </div>
      <div class="bg-shape-14"></div>
    </div>

    <div class="collapse" id="collapseTimer">

      <div class="row position-relative justify-content-center">

        <div class="progress" style="height: 40px; width: 50%;">
            <div class="progress-bar progress-bar-striped active" role="progressbar"
            aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:40%;">
            40%
            </div>
        </div>
        
        <div class="wrapper">
          <div class="tabs_wrap">
            <ul>
              <li data-tabs="active" class="active">Active</li>
              <li data-tabs="calm">Calm</li>
              <li data-tabs="pause">Pause</li>
            </ul>
          </div>
        </div>
      </div>  
    </div>

    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">Ã—</span>
            </button>
          </div>
          <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
          <div class="modal-footer">
            <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
            <a class="btn btn-primary" onclick="logout()">Logout</a>
          </div>
        </div>
      </div>
    </div>

    <footer class="footer-area-l-12 position-relative" style="text-align:center;">
    </footer>
