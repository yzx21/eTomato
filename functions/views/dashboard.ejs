                            () => {});
                    },
                    error: function (error) {
                        new Toasteur().error(error.responseText, 'Error!', () => {});
                    },
                });
            } else {}
        }


        function addAnote() {
            if (document.getElementById('addnote').innerText == "+ Add a note") {
                $('#composingSec').collapse("show")
