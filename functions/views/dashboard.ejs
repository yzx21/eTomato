        <!-- Begin Page Content -->
        <div class="container-fluid">
          <% if(Object.keys(passed_models).length==0) { %>
            <div class="container-fluid mt-100">
              <div class="row">
                <div class="col-md-12">
                  <div class="card">
                    <div class="card-header">
                      <h5>Subscription</h5>
                    </div>
                    <div class="card-body cart">
                      <div class="col-sm-12 empty-cart-cls text-center">
                        <i class="fas fa-fw fa-cubes fa-10x"></i>
                        <h3><strong>You don't have any stream yet</strong></h3>
                        <h4>Start to create your first stream now</h4> <a href="./new_model"
                          class="btn btn-primary cart-btn-transform m-3" data-abc="true">Create a new stream</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <% } else { %>
              <% const values=Object.values(passed_models); const keys=Object.keys(lineDataPoints); for(var i=0; i<values.length; i++) { %>
                <div class="col-md-9">
                  <div class="osahan-account-page-right shadow-sm bg-white p-4 h-100">
                    <div class="tab-content" id="myTabContent">
                      <div class="tab-pane  fade  active show" id="orders" role="tabpanel" aria-labelledby="orders-tab">
                        <% if(values[i].public) {%>
                          <span class="float-right text-info">[Public]<i
                            class="icofont-check-circled text-success"></i>
                          </span>
                        <% } else {%>
                          <span class="float-right text-info">[Private]<i
                            class="icofont-check-circled text-info"></i>
                          </span>
                        <% } %>
                        <% if(values[i].used) { %>
                          <span class="float-right text-info">[Used]<i
                            class="icofont-check-circled text-success"></i>
                          </span>
                        <% } else {%>
                          <span class="float-right text-info">[Unused]<i
                            class="icofont-check-circled text-info"></i>
                          </span>
                        <% } %>

                        <h4 class="font-weight-bold mt-0 mb-4">Streams created by me
                          <% if (values[i].coAuthorCnt != 0) { %>
                            and (an)other <%= values[i].coAuthorCnt %> author(s)
                          <% } %>
                        </h4>
                        <div class="bg-white card mb-4 order-list shadow-sm">
                          <div class="gold-members p-4">
                            <div class="media">
                              <i class="fas fa-fw fa-cubes fa-5x" style="color:blue"></i>
                              <div class="media-body">
                                <span class="float-right text-info">Created at <%= values[i].create_time %> <i
                                      class="icofont-check-circled text-success"></i>
                                </span>
                                <h6 class="mb-2">
                                  <p class="text-black">
                                    <%= values[i].model_name %> </a>
                                    <p class="text-black">
                                      <%= values[i].stocks %> </a>    
                                </h6>
                                <p class="text-gray mb-1"><i class="icofont-location-arrow"></i>
                                  <%= values[i].model_description %> </a>
                                    <span class="float-right">Subscription: <%= values[i].like_cnt %> <i
                                      class="icofont-check-circled text-success"></i>
                                    </span>
                                    <span class="float-right">Score: <%= values[i].score %>/<i
                                      class="icofont-check-circled text-success"></i>
                                    </span>
                                    <% if (keys.includes(values[i].model_id) ) { %> 
                                      <hr>
                                      <% for(let j=0; j<=values[i].yList.length; j++) { %>
                                        <div id="wrapper" class="<%=values[i].model_id+j%>" hidden="true">    
                                            <canvas class="my-4 w-100 h-25" id="<%=values[i].model_id+j%>" height="50" ></canvas>
                                        </div>
                                      <% } %>
                                      <canvas class="my-4 w-100" id="<%=values[i].model_id%>" width="900" height="150"></canvas>
                                      <div class="custom-control custom-switch float-right">
                                        <input type="checkbox" class="custom-control-input" id="customSwitches<%=i%>" onclick="toogleCheckbox(this, '<%=values[i].model_id%>', '<%=values[i].yList.length%>')">
                                        <label class="custom-control-label" for="customSwitches<%=i%>"></label>
                                    </div>
                                    <% } %>
                                    <hr>
                                    <div style="text-align: center">
                                      <div class="float-left">
                                        <% href="./model_output?model_id=" + values[i].model_id%>
                                          <a class="btn btn-sm btn-outline-primary" href=<%=href%>> <i
                                              class="icofont-refresh"></i>
                                            Stream Output</a>
                                      </div>
                                      model_id: <p id="<%=values[i].model_id%>_cpy" onclick="copy('<%=values[i].model_id%>'+'_cpy')"><mark><%= values[i].model_id %></mark></p>
                                      <div class="float-right" style="vertical-align: top">
                                        <% href="./edit_model?model_id=" + values[i].model_id%>
                                          <a class="btn btn-sm btn-primary" href=<%=href%>> <i
                                              class="icofont-refresh"></i>
                                            EDIT</a>
                                      </div>
                                    </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <% } %>
                  <% } %>
        </div>
        <!-- End of Main Content -->

        <!-- Footer -->
        <footer class="sticky-footer bg-white">
          <div class="container my-auto">
            <div class="copyright text-center my-auto">
              <span>Copyright &copy; Armadillo </span>
            </div>
          </div>
        </footer>
        <!-- End of Footer -->

      </div>
      <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
      <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
      aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">Ã—</span>
            </button>
          </div>
          <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
          <div class="modal-footer">
            <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
            <a class="btn btn-primary" onclick="LogoutFunction()">Logout</a>
          </div>
        </div>
      </div>
    </div>

    <script>
      function LogoutFunction() {
        console.log("Clearing session");
        fetch("./logout", {
          method: "GET",
        })
          .then(response => {
            console.log("window.location.href = ");
            window.location.href = "./index";
          });
        sessionStorage.setItem("session", "");
      }

      function clearUnread() {
        fetch("./clear_unread", {
          method: "GET"
        })
        .then(response => {
          unread = []
          document.getElementById("unread_cnt_id").textContent = "";
        });
      }

      function copy(model_id) {
        var copyText = document.getElementById(model_id).innerText;
        var elem = document.createElement("textarea");
        document.body.appendChild(elem);
        elem.value = copyText;
        elem.select();
        document.execCommand("copy");
        document.body.removeChild(elem);
        alert("Copied!"); 
      }

      function loadChart() {
            const firebaseConfig = {
              apiKey: "AIzaSyCWtGfTpToGgibo6nesNnMGXreMgBNZdDo",
              authDomain: "modelcreator-cdd8f.firebaseapp.com",
              projectId: "modelcreator-cdd8f",
              storageBucket: "modelcreator-cdd8f.appspot.com",
              messagingSenderId: "810288170650",
              appId: "1:810288170650:web:da961e0e607a7ae2e14123",
              measurementId: "G-X6WPSQ0HTH"
            };
            try {
              firebase.initializeApp(firebaseConfig);
            } catch {
            }
            firebase.analytics().logEvent('dashboard');
            
            var lineDP = JSON.parse('<%- JSON.stringify(lineDataPoints) %>');
            var mdIds = Object.keys(lineDP);
            var psdMdl = '<%- JSON.stringify(passed_models) %>'.replaceAll('""', '"').replaceAll('/g', '"');
            var mds = JSON.parse(psdMdl);
            for(let i=0; i<mds.length; i++) {
                var id = mds[i]['model_id'];
                var basePointsX = lineDP[id][0];
                var basePointsY = lineDP[id][1];
                const canvas = document.getElementById(id);
                if (!canvas) continue
                const ctx = canvas.getContext('2d');
                // eslint-disable-next-line no-unused-vars

                var datasets = [];
                datasets.push({
                            label: "prediction",
                            data: basePointsY,
                            lineTension: 0,
                            backgroundColor: 'transparent',
                            borderColor: '#007bff',
                            borderWidth: 1,
                            pointBackgroundColor: '#007bff',
                            pointRadius: 1,
                            pointHoverRadius: 5
                        })
                var yList = mds[i]['yList'];
                for( let j=0; j<yList.length; j++) {
                    var name = yList[j][0];
                    var xs = yList[j][1];
                    var ys = yList[j][2];
                    datasets.push({
                        label: name,
                        data: ys,
                        backgroundColor: 'transparent',
                        borderColor: '#808080',
                        borderWidth: 0.5,
                        pointBackgroundColor: '#808080',
                        pointRadius: 1,
                        pointHoverRadius: 5
                    })
                } 
                var myChart = new Chart(canvas, {
                    type: 'line',
                    data: {
                        labels: xs,
                        datasets: datasets
                    },
                    options: {
                        scales: {
                            yAxes: [{
                                ticks: {
                                    beginAtZero: false
                                }
                            }]
                        },
                        legend: {
                            display: false
                        }
                    }
                });


                var datasets2 = [];
                var yList = mds[i]['yList'];
                for( let j=0; j<yList.length; j++) {
                    var name = yList[j][0];
                    var xs = yList[j][1];
                    var ys = yList[j][2];
                    datasets2.push({
                        label: name,
                        data: ys,
                        backgroundColor: 'transparent',
                        borderColor: '#808080',
                        borderWidth: 0.5,
                        pointBackgroundColor: '#808080',
                        pointRadius: 1,
                        pointHoverRadius: 5
                    })
                }
                const canvas2 = document.getElementById(id+"0");
                var myChart = new Chart(canvas2, {
                    type: 'line',
                    data: {
                        labels: xs,
                        datasets: datasets2
                    },
                    options: {
                        scales: {
                            yAxes: [{
                                ticks: {
                                    beginAtZero: false
                                }
                            }]
                        },
                        legend: {
                            display: false
                        }
                    }
                });


                var datasets3 = [];
                datasets3.push({
                            label: "prediction",
                            data: basePointsY,
                            lineTension: 0,
                            backgroundColor: 'transparent',
                            borderColor: '#007bff',
                            borderWidth: 1,
                            pointBackgroundColor: '#007bff',
                            pointRadius: 1,
                            pointHoverRadius: 5
                        })
                const canvas3 = document.getElementById(id+yList.length);
                var myChart = new Chart(canvas3, {
                    type: 'line',
                    data: {
                        labels: xs,
                        datasets: datasets3
                    },
                    options: {
                        scales: {
                            yAxes: [{
                                ticks: {
                                    beginAtZero: false
                                }
                            }]
                        },
                        legend: {
                            display: false
                        }
                    }
                });
            }
       }
 
      function toogleCheckbox(item, modelId, cnt) {
          if(item.checked) {
              for(let j=0; j<=cnt; j++) {
                  var name = modelId + j.toString();
                  var chart = document.getElementsByClassName(name);
                  chart.wrapper.hidden=false
              }
          } else {
              for(let j=0; j<=cnt; j++) {
                  var name = modelId + j.toString();
                  var chart = document.getElementsByClassName(name);
                  chart.wrapper.hidden=true
              }
          }
      }
    </script>

