            if (!tomatoType || tomatoType.length == 0) {
                tomatoType = "No type";
            }
            $.ajax({
                url: "./saveNotes",
                type: "POST",
                data: {
                    // checkedValue: checkedValue,
                    tomatoType: tomatoType,
                    notes: notes,
                },
                success: function (result) {
                    new Toasteur().success("Note saved", '',
                        () => {});
                },
                error: function (error) {
                    new Toasteur().error(error.responseText, 'Error!', () => {});
